    "on-finished": "~2.3.0",
    "on-headers": "~1.0.2"
  },
  "devDependencies": {
    "eslint": "6.7.2",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-markdown": "1.0.1",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
